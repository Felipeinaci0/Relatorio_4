.data
.text

# a) 324 AND 100
li $t0, 324    # $t0 = 324
li $t1, 100    # $t1 = 100

# Realiza a operação AND para a)
and $t2, $t0, $t1  # $t2 = $t0 AND $t1

# b) 2018 AND 1970
li $t3, 2018   # $t3 = 2018
li $t4, 1970   # $t4 = 1970

# Realiza a operação AND para b)
and $t5, $t3, $t4  # $t5 = $t3 AND $t4

# c) 33 AND 66
li $t6, 33    # $t6 = 33
li $t7, 66    # $t7 = 66

# Realiza a operação AND para c)
and $s0, $t6, $t7  # $s0 = $t6 AND $t7
